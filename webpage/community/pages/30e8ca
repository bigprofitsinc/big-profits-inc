				&lt;a href=&quot;http://vanilla.stackideas.com/community/pages/30&quot;&gt;עסקים&lt;/a&gt;
			&lt;/h2&gt;

			&lt;ul class=&quot;g-list-inline g-list-inline--dashed es-profile-header__meta t-lg-mt--md&quot;&gt;
				&lt;li class=&quot;t-text--muted&quot;&gt;
					&lt;a href=&quot;http://vanilla.stackideas.com/general&quot;&gt;
						&lt;i class=&quot;fa fa-folder&quot;&gt;&lt;/i&gt;&amp;nbsp; General					&lt;/a&gt;
